	keywords = {Decision support, Expert, Farmer, Local knowledge, Questionnaire survey, Stakeholder participation, Uncertainty, Water quality},
	pages = {76--85},
	file = {ScienceDirect Snapshot:/Users/corywhitney/Zotero/storage/JQZMRPNC/S1364815211002118.html:text/html}
