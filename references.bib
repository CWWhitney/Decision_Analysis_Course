	author = {Whitney, Cory W. and Luedeling, Eike and Gebauer, J. and Nyamukuru A, Antonia and Tabuti, John R. S. and Kehlenbeck, K.},
	year = {2016},
	keywords = {food security, agrobiodiversity, Amaranthus dubius, Amaranthus hybridus, Amaranthus spinosus, Antiaris toxicaria, Artocarpus heterophyllus, cluster analysis, Coffea arabica, Euclidean distance, Grevillea robusta, Gynandropsis gynandra, Gynura scandens, Hibiscus acetosella, homegarden, indigenous knowledge, Mahalanobis distance, Pielou’s J’., Saccharum officinarum, Shannon H’, Solanum torvum},
