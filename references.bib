	year = {2019},
	keywords = {Burkina Faso, Decision analysis, Monte-Carlo simulation, Reservoir sedimentation, Uncertainty assessment},
	pages = {164--175},
	file = {ScienceDirect Snapshot:/Users/corywhitney/Zotero/storage/M53BMU4D/S1364815218301129.html:text/html;Lanzanova et al_2019_Improving development efficiency through decision analysis.pdf:/Users/corywhitney/Zotero/storage/2QMEFYDK/Lanzanova et al_2019_Improving development efficiency through decision analysis.pdf:application/pdf;Lanzanova et al. - 2019 - Improving development efficiency through decision .pdf:/Users/corywhitney/Zotero/storage/HZEC9Q56/Lanzanova et al. - 2019 - Improving development efficiency through decision .pdf:application/pdf}
